#!/usr/bin/env python

import apache_beam
#import create_pipeline
import unittest

from options import SetPipelineOptions

class TestCreatePipeline(unittest.TestCase):
    def __init__(): 
        pass     

    def test_pipeline_create():
        pipeline = create_pipeline.CreatePipeline()
        return pipeline
    
class TestOptions(unittest.TestCase):
    def setUp(self):
        return None 

    def test_set_options(self):
        options = SetPipelineOptions() 
        self.assertIsInstance(options, apache_beam.utils.pipeline_options.PipelineOptions)
    
class TestStreamsRead(unittest.TestCase):
    def __init__():
        pass

class TestTracksRead(unittest.TestCase):
    def __init__():
       pass 

class TestUsersRead(unittest.TestCase):
    def __init__():
        pass

suite = unittest.TestLoader().loadTestsFromTestCase(TestOptions)
unittest.TextTestRunner(verbosity=2).run(suite)
